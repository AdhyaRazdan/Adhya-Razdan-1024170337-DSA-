#include <iostream>
using namespace std;
class Node {
public:
	int data;
	Node* left;
	Node* right;
	Node(int new_data) {
		data=new_data;
		left=right=nullptr;
	}
};

void inorder(Node* root){   //LPR
    if(root==NULL){
        return;
    }
    inorder(root->left);
    cout<<root->data<<" ";
    inorder(root->right);
}
Node* insert(Node* root, int num){
    if(root==NULL){
        return new Node(num);
    }
    if(num < root->data){
        root->left=insert(root->left,num);
    }
    else{
        root->right=insert(root->right,num);
    }
    return root;
}
int main() {
	Node* root=new Node(50);
	root->left=new Node(30);
	root->right=new Node(70);
	root->left->left=new Node(20);
	root->left->right=new Node(40);
	root->right->left=new Node(60);
	root->right->right=new Node(80);
	
	cout<<"Before: ";
	inorder(root);
	cout<<endl;
	Node* res=insert(root,100);
	
	cout<<"After: ";
	inorder(root);
	return 0;
}
