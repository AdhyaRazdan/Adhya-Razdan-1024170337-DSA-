#include <iostream>
#include <unordered_set>
using namespace std;

bool hasDuplicate(int nums[], int n) {
    unordered_set<int> s;
    for (int i = 0; i < n; i++) {
        if (s.count(nums[i]))   return true;
        s.insert(nums[i]);        
    }
    return false;                 
}

int main() {
    int nums[] = {1, 2, 3, 1};
    int n = 4;
    if (hasDuplicate(nums, n))
        cout << "true";
    else
        cout << "false";
}
